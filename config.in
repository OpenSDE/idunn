#Description: Idunn, early recovery system

#
# Generic Settings
#
SDECFGSET_CROSSBUILD='1'
SDECFGSET_IMAGE='initramfs'
SDECFGSET_OPT='size'
SDECFGSET_CONTINUE_ON_ERROR_AFTER=9

SDECFGSET_INIT='none'
SDECFGSET_LIBC='uclibc'

SDECFGSET_EXPERT=1
SDECFGSET_FILTER_NODEVEL=1

# no NLS
SDECFGSET_DISABLE_NLS='1'

#
# Package specific settings
#

# enable ccache
SDECFGSET_PKG_CCACHE_USEIT=1

# just C/C++
SDECFGSET_PKG_GCC_JAVA=0
SDECFGSET_PKG_GCC_F77=0
SDECFGSET_PKG_GCC_OBJC=0

# busybox
SDECFGSET_PKG_BUSYBOX_SYMLINKS=1
SDECFGSET_PKG_BUSYBOX_DEFCONFIG=0
SDECFGSET_PKG_BUSYBOX_SUSv2=0

menu_begin MENU_MNEMOSYNE 'Mnemosyne Options'
